{
  "env": {
    "browser": true
  },
  "extends": [
    "airbnb-base",
    "prettier"
  ],
  "rules": {
    "no-nested-ternary": "off",
    "no-console": "off",
    "no-restricted-syntax": "off",
    "no-continue": "off",
    "no-param-reassign": "off",
    "import/prefer-default-export": "off",
    "prettier/prettier": [
      "error"
    ]
  },
  "plugins": [
    "prettier"
  ]
}